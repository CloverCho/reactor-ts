{
    "name": "wirewrap",
    "version": "0.0.1",
    "description": "A Reactive Framework for IoT",
    "main": "lib/index.js",
    "dependencies": {
        "documentation": "^9.3.1",
        "flow": "^0.2.3",
        "got": "^9.6.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-flow": "^7.0.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "flow-bin": "^0.93.0",
        "jest": "^24.7.1"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rm -rf lib && babel -D src -d lib && npm run doc",
        "doc": "documentation build src/*.js -f html -o docs --sort-order alpha",
        "documentation": "documentation",
        "debug": "babel-node app.js",
        "flow": "flow",
        "flow-typed": "flow-typed",
        "jest": "jest",
        "lint": "eslint src",
        "test": "node_modules/.bin/flow  && node_modules/.bin/jest --coverage"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "canopy"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:icyphy/accessor-flow.git"
    },
    "keywords": [
        "IoT",
        "framework",
        "timing",
        "determinism",
        "actors"
    ],
    "author": {
        "name": "Marten Lohstroh",
        "email": "marten@berkeley.edu",
        "url": "http://people.eecs.berkeley.edu/~marten"
    },
    "contributors": [
        "Ben Zhang",
        "Andres Goens"
    ],
    "license": "MIT"
}
