{
    "name": "wirewrap",
    "version": "0.0.1",
    "description": "A Reactive Framework for IoT",
    "main": "lib/reactor.js",
    "dependencies": {
        "got": "^9.6.0",
        "heap": "^0.2.6",
        "microtime": "^3.0.0",
        "nanotimer": "^0.3.15"
    },
    "devDependencies": {
        "@babel/cli": "^7.4.3",
        "@babel/core": "^7.4.3",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.4.3",
        "@babel/preset-flow": "^7.0.0",
        "@types/jest": "^24.0.15",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "jest": "^24.8.0",
        "ts-jest": "^24.0.2",
        "typedoc": "^0.15.5",
        "typescript": "^3.5.3"
    },
    "files": [
        "lib"
    ],
    "scripts": {
        "build": "rm -rf lib && babel -D src -d lib && npm run doc",
        "doc": "typedoc --out docs src",
        "test": "jest --coverage"
    },
    "jest": {
        "testPathIgnorePatterns": [
            "node_modules"
        ],
        "roots": [
            "<rootDir>/src",
            "<rootDir>/__tests__"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        }
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:lhstrh/wirewrap.git"
    },
    "keywords": [
        "IoT",
        "framework",
        "timing",
        "determinism",
        "actors"
    ],
    "author": {
        "name": "Marten Lohstroh",
        "email": "marten@berkeley.edu",
        "url": "http://people.eecs.berkeley.edu/~marten"
    },
    "contributors": [
        "Andres Goens",
        "Matt Weber",
        "Ben Zhang"
    ],
    "license": "MIT"
}
