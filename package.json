{
    "name": "accessor-flow",
    "version": "0.0.1",
    "description": "Actors for Composable Reactive Systems.",
    "main": "lib/index.js",
    "dependencies": {
        "flow": "^0.2.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-flow": "^7.0.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "jest": "^24.0.0"
    },
    "files": [
        "lib"
    ],
    "scripts": {
